<h1>Version Logs</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Event</th>
      <th>Who Done the update</th>
      <th>Created At</th> 
    </tr>
  </thead>

  <tbody>
        <% @users.each do |user| %>
            <% user.versions.each do |v| %>
                <tr>
                    <td><%= v.item_id %></td>
                    <td><%= v.event %></td>
                    <td><%= v.whodunnit %></td>
                    <td><%= v.created_at %></td>
                </tr>
            <% end %> 
        <% end %> 
  </tbody>
</table>
